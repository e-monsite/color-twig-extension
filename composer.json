{
    "name": "e-monsite/color-twig-extension",
    "description": "Manipulate colors with twig",
    "type": "library",
    "keywords": [
        "e-monsite",
        "emonsite",
        "color",
        "twig"
    ],
    "require": {
        "php": "^7.3|^8.0",
        "mikealmond/color": "^0.1.3",
        "twig/twig": "^2.12|^3.0",
        "marcovo/shade-blend-convert": "^1.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Emonsite\\ColorTwigExtension\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Emonsite\\ColorTwigExtension\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    }
}
